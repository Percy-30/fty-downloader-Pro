{
  "version": 2,
  "buildCommand": "cd app-descargador && npm run build",
  "devCommand": "cd app-descargador && npm run dev",
  "installCommand": "cd app-descargador && npm install",
  "framework": "nextjs",
  "outputDirectory": "app-descargador/.next",
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        { "key": "Content-Type", "value": "application/xml" }
      ]
    }
  ]
}
